{
    "name": "sunnyflail/container",
    "description": "A simple Dependency Injection container",
    "type": "library",
    "require": {
        "php" : ">=7.1",
        "psr/container": "^2.0",
        "sunnyflail/typestring": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "authors": [
        {
            "name": "SunnyFlail",
            "email": "sunny.flail@gmail.com"
        }
    ],
    "autoload" : {
        "psr-4": {
            "SunnyFlail\\DI\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\" : "tests"
        }
    },
    "config": {
        "classmap-authoritative":true
    },
    "keywords": [
        "PSR-11",
        "container"
    ]
}
