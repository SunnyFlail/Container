{
    "name": "sunnyflail/container",
    "description": "A simple Dependency Injection container",
    "type": "library",
    "require": {
        "psr/container": "^2.0",
        "sunnyflail/typestring": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "authors": [
        {
            "name": "SunnyFlail",
            "email": "sunny.flail@gmail.com"
        }
    ],
    "autoload" : {
        "psr-4": {
            "SunnyFlail\\DI\\" : "src"
        }
    },
    "config": {
        "classmap-authoritative":true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/SunnyFlail/Typestring"
        }
    ]
}
